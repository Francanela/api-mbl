{
    "version": 2,
    "builds": [
      {
        "src": "/",
        "use": "@vercel/node",
        "config": {
          "buildCommand": "npx prisma generate && next build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/main.js",
        "methods": [
            "GET",
            "POST",
            "PUT",
            "DELETE"
        ]
      }
    ]
}