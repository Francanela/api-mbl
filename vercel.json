{
    "version": 2,
    "builds": [
      {
        "src": "src/main.ts",
        "use": "@vercel/node",
        "config": {
          "buildCommand": "npx prisma generate && next build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/main.ts"
      }
    ]
}